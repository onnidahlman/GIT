# Vaihda haara päähaaraan (esim. main)
git checkout main

# Luo uusi haara feature/create-awesome ja vaihda siihen
git checkout -b feature/create-awesome

# Tee muutos tiedostoon hello_world.py
# Avaa tiedosto tekstieditorilla ja lisää rivi:
# print("Hello from feature")
# Tallenna tiedosto

# Lisää muutokset ja tee commit
git add hello_world.py
git commit -m "Add print statement to hello_world.py in feature/create-awesome branch"

git push origin feature/create-awesome

# Vaihda haara päähaaraan (esim. main)
git checkout main

# Luo uusi haara release ja vaihda siihen
git checkout -b release

# Tee muutos tiedostoon hello_world.py
# Avaa tiedosto tekstieditorilla ja lisää rivi:
# print("Hello from release")
# Tallenna tiedosto

# Lisää muutokset ja tee commit
git add hello_world.py
git commit -m "Add print statement to hello_world.py in release branch"

git push origin release
